#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/cwebp-bin@6.1.2/node_modules/cwebp-bin/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/cwebp-bin@6.1.2/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/cwebp-bin@6.1.2/node_modules/cwebp-bin/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/cwebp-bin@6.1.2/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cwebp-bin@6.1.2/node_modules/cwebp-bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../cwebp-bin@6.1.2/node_modules/cwebp-bin/cli.js" "$@"
fi
