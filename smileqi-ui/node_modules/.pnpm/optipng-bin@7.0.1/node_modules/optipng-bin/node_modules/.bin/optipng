#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/optipng-bin@7.0.1/node_modules/optipng-bin/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/optipng-bin@7.0.1/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/optipng-bin@7.0.1/node_modules/optipng-bin/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/optipng-bin@7.0.1/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
