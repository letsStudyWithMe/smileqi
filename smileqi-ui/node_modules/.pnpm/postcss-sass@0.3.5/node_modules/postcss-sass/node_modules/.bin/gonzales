#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules/gonzales-pe/bin/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules/gonzales-pe/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules/gonzales-pe/bin/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules/gonzales-pe/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../gonzales-pe@4.3.0/node_modules/gonzales-pe/bin/gonzales.js" "$@"
else
  exec node  "$basedir/../../../../../gonzales-pe@4.3.0/node_modules/gonzales-pe/bin/gonzales.js" "$@"
fi
