#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/bin-wrapper-china@0.1.0/node_modules/bin-wrapper-china/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/bin-wrapper-china@0.1.0/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/bin-wrapper-china@0.1.0/node_modules/bin-wrapper-china/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/bin-wrapper-china@0.1.0/node_modules:/mnt/d/MyProject/smileqi-ui/smileqi-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../bin-wrapper/inject-env.js" "$@"
else
  exec node  "$basedir/../bin-wrapper/inject-env.js" "$@"
fi
